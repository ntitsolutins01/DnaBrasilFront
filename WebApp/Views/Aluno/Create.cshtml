                                                           title="Por favor informe a data de nascimento."
                                                           style="z-index: auto;"
                                                           data-plugin-datepicker data-date-format="dd/mm/yyyy" data-plugin-options='{"language": "pt-BR"}'
                                                           placeholder="__/__/____" class="form-control startdate"
                                                           class="form-control">
                                                </div>
                                            </div>
                                        </div>
                                       @*  <div class="form-group">
                                            <label class="col-sm-2 control-label">Estado/Profissional</label>
                                            <div class="col-sm-4">
                                                <select class="form-control populate select2" name="ddlEstado" id="ddlEstado"
                                                        title="Por favor selecione um estado." required
                                                        asp-for="EstadoId" asp-items="Model.ListEstados">
                                                    <option value="">Selecionar Estado</option>
                                                </select>
                                            </div>

                                            <label class="col-sm-2 control-label">Profissional </label>
                                            <div class="col-sm-4">
                                                <select class="form-control populate select2" name="ddlProfissional" id="ddlProfissional"
                                                        title="Por favor selecione um profissional." required
                                                asp-for="CodProfissional" asp-items="Model.ListProfissional">
                                                    <option value="">Selecionar Profissional</option>
                                                </select>
                                            </div>
                                        </div> *@
                                        <div class="form-group">
                                            <label class="col-sm-1 control-label">Nome Mãe</label>
                                            <div class="col-sm-5">
                                                <input type="text" name="nomeMae" maxlength="150"class="form-control" />
                                            </div>
                                            <label class="col-sm-1 control-label">Nome Pai</label>
                                            <div class="col-sm-5">
                                                <input type="text" name="nomePai" maxlength="150"class="form-control" />
                                            </div>
                                        </div>
                                    <div class="form-group">
	                                    <label class="col-sm-1 control-label text-sm-right pt-2">Estado <span class="required">*</span></label>
	                                    <div class="col-md-2">
		                                    <select class="form-control populate select2" name="ddlEstado" id="ddlEstado"
		                                            title="Por favor informe um Estado." required
		                                            asp-for="EstadoId" asp-items="Model.ListEstados">
			                                    <option value="">Selecionar Estado</option>
		                                    </select>
	                                    </div>
	                                    <label class="col-sm-1 control-label text-sm-right pt-2">Município <span class="required">*</span></label>
	                                    <div class="col-md-3">
		                                    <select class="form-control populate select2" name="ddlMunicipio" id="ddlMunicipio"
		                                            title="Por favor informe um Municipio." required
		                                            asp-for="MunicipioId" asp-items="Model.ListMunicipios">
			                                    <option value="">Selecionar Município</option>
		                                    </select>
	                                    </div>
	                                    <label class="col-sm-2 control-label text-sm-right pt-2">Localidade <span class="required">*</span></label>
	                                    <div class="col-md-3">
		                                    <select class="form-control populate select2" name="ddlLocalidade" id="ddlLocalidade"
		                                            title="Por favor informe o localidade." required
		                                            asp-for="LocalidadeId" asp-items="Model.ListLocalidades">
			                                    <option value="">Selecionar Localidade</option>
		                                    </select>
	                                    </div>
                                    </div>
                                    <div class="form-group row">
	                                    <label class="col-sm-3 control-label text-sm-right pt-2">Profissional <span class="required">*</span></label>
	                                    <div class="col-md-4">
		                                    <select class="form-control populate select2" name="ddlLocalidade" id="ddlLocalidade"
		                                            @* title="Por favor informe o localidade." required *@
                                                        asp-for="ProfissionalId" asp-items="Model.ListProfissionals">
			                                    <option value="">Selecionar Localidade</option>
		                                    </select>
	                                    </div>
                                    </div>
                                        <div class="form-group">
                                            <label class="col-sm-1 control-label">Endereço</label>
                                            <div class="col-sm-6">
                                                <input type="text" id="endereco" name="endereco" maxlength="200" class="form-control"/>
                                            </div>  
                                            <label class="col-sm-1 control-label">Número</label>
                                            <div class="col-sm-1">
                                                <input type="number" id="numero" name="numero" maxlength="4" class="form-control"/>
                                            </div>
                                            <label class="col-sm-1 control-label">Cep</label>
                                            <div class="col-sm-2">
                                                <input type="text" id="cep" name="cep" maxlength="9" class="form-control"/>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-1 control-label">Bairro </label>
	                                        <div class="col-sm-3">
		                                        <input type="bairro" name="bairro" maxlength="50" class="form-control"/>
	                                        </div>
	                                        <label class="col-lg-1 control-label text-lg-right pt-2">Telefone</label>
	                                        <div class="col-lg-3">
		                                        <div class="input-group">
			                                        <span class="input-group-addon">
				                                        <i class="fa fa-phone"></i>
			                                        </span>
			                                        <input id="numTelefone" name="numTelefone"class="form-control"
			                                               maxlength="14">
		                                        </div>
	                                        </div>
	                                        <label class="col-sm-1 control-label">Celular </label>
	                                        <div class="col-sm-3">
		                                        <div class="input-group">
			                                        <span class="input-group-addon">
				                                        <i class="fa fa-phone"></i>
			                                        </span>
			                                        <input id="numCelular" name="numCelular"class="form-control"
			                                               maxlength="14">
		                                        </div>
	                                        </div>
	                                        
                                        </div>
                                        @* <div class="form-group">
                                        <label class="col-sm-1 control-label">Facebook</label>
                                        <div class="col-sm-5">
                                        <div class="input-group">
                                        <span class="input-group-addon">
                                        <i class="fa fa-facebook"></i>
                                        </span>
                                        <input type="facebook" name="facebook" maxlength="100" class="form-control" required />
                                        </div>
                                        </div>
                                        <label class="col-sm-1 control-label">Twitter</label>
                                        <div class="col-sm-5">
                                        <div class="input-group">
                                        <span class="input-group-addon">
                                        <i class="fa fa-twitter"></i>
                                        </span>
                                        <input type="email" name="email" class="form-control" required />
                                        </div>
                                        </div>
                                        </div> *@

                                        <div class="form-group">
                                            <label class="col-sm-3 control-label pull-left">Cadastro ativo para login?</label>
                                            <div class="col-sm-1">
                                                <div class="switch switch-sm switch-success">
                                                    <input type="checkbox" id="ativo" name="ativo" data-plugin-ios-switch checked="checked" />
                                                </div>
                                            </div>
                                            <label class="col-sm-3 control-label">Cadastro habilitado a operar?</label>
                                            <div class="col-sm-1">
                                                <div class="switch switch-sm switch-success">
                                                    <input type="checkbox" id="habilitado" name="habilitado" data-plugin-ios-switch checked="checked" />
                                                </div>
                                            </div>
                                        </div>
                                        <blockquote class="danger">
                                            <p>Deficiências</p>
                                            <small>Caso possuir alguma deficiências todos os campos devem ser obrigatórios </small>
                                        </blockquote>
                                        <div class="form-group">
                                            <label class="col-sm-2 control-label">Possui deficiência?</label>
                                            <div class="col-sm-10">
                                                <div class="radio-custom radio-success">
                                                    <input type="radio" id="sim" name="rdbDeficiencia" value="sim">
                                                    <label for="radioExample1">Sim</label>
                                                </div>

                                                <div class="radio-custom radio-danger">
                                                    <input type="radio" id="nao" name="rdbDeficiencia" value="nao" checked>
                                                    <label for="radioExample2">Não</label>
                                                </div>

                                            </div>
                                        </div>
                                        <div id="divDeficiencia" name="divDeficiencia">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label">Deficiência</label>
                                                <div class="col-sm-4">
                                                    <select class="form-control populate select2" name="ddlDeficiencia" id="ddlDeficiencia"
	                                                        style="width: 100%" title="Por favor selecione o turno."
                                                            asp-for="DeficienciaId" asp-items="Model.ListDeficiencias">
                                                        <option value="">Selecionar Deficiência</option>
	                                                </select>
                                                </div>
	                                            <div class="col-sm-1">
		                                            <a type="button" class="ml-xs btn btn-success" href='javascript:(crud.AddDeficiencia())'>
			                                            <i class="fa fa-plus"></i>
		                                            </a>
	                                            </div>
	                                            <div class="col-sm-5 pull-right">
                                                    <a data-toggle="modal" data-target="#mdDeficiencia" class="btn btn-primary pull-right"><i class="fa fa-plus"></i> Nova Deficiência </a>
	                                            </div>
                                            </div>
	                                        <div class="form-group">
                                                <input type="hidden" name="arrDeficiencias" />
		                                        <label class="col-sm-2 control-label">Adicionados</label>
		                                        <div class="col-sm-10">
                                                    <table class="table table-bordered table-striped mb-0" id="deficienciaDataTable">
				                                        <thead>
				                                        <tr>
					                                        <th>Id</th>
					                                        <th>Nome Deficiência</th>
					                                        <th>Ação</th>
				                                        </tr>
				                                        </thead>
			                                        </table>
		                                        </div>
	                                        </div>
                                        </div>

                                    </div>
                                    <footer class="panel-footer">
                                        <div class="row">
                                            <div class="col-md-9 col-md-offset-3">
                                                <button type="submit" class="btn btn-danger pull-right" style="margin-left: 5px;"><i class="fa fa-remove"></i> &nbsp;&nbsp;Cancelar</button>
                                                <button type="submit" class="btn btn-success pull-right" style="margin-left: 5px;"><i class="fa fa-save"></i> &nbsp;&nbsp;Salvar</button>

                                            </div>
                                        </div>
                                    </footer>
                                </section>
	                        }
                        </div>
                    </div>
                </div>
                <div id="dependencias" class="tab-pane">
                    <div class="row">
                        <div class="col-md-12">
                            @using (@Html.BeginForm("CreateDependencia", "Aluno", FormMethod.Post, new { id = "formMatriculaAluno", @class = "form-horizontal" }))
	                        {
                                <section class="panel">
                                    <div class="panel-body">
                                        <div class="form-group">
                                            <div class="col-sm-9">
                                                <div class="radio-custom radio-success">
                                                    <input type="radio" id="sim" name="rdbAutorizacao" value="sim">
                                                    <label for="radioExample1">
                                                        Autorizo o mesmo a se ausentar das dependências do
                                                        Centro Olímpico e Paralímpico sem a presença de um responsável após o
                                                        término de suas atividades.
                                                    </label>
                                                </div>

                                                <div class="radio-custom radio-danger">
                                                    <input type="radio" id="nao" name="rdbAutorizacao" value="nao">
                                                    <label for="radioExample2">
                                                        Não autorizo o mesmo a se ausentar das dependências do
                                                        Centro Olímpico e Paralímpico sem a presença de um responsável após o término
                                                        de suas atividades.
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

