                                            <div class="col-sm-3">
                                                <input type="text" id="serie" name="serie" class="form-control"/>
                                            </div>
                                            <label class="col-sm-1 control-label">Ano </label>
                                            <div class="col-sm-2">
                                                <input type="text" id="ano" name="ano" class="form-control"/>
                                            </div>
