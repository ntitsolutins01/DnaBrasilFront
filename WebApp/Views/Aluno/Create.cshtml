		                                    <div class="input-group">
			                                    <span class="input-group-addon">
				                                    <i class="fa fa-envelope"></i>
			                                    </span>
			                                    <input type="text" name="email" maxlength="100" class="form-control"/>
		                                    </div>
	                                    </div>
                                    </div>
