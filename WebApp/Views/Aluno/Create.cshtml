                                            <div class="col-sm-1">
                                                <div class="radio-custom radio-primary">
                                                    <input id="awesome" name="porto_is" type="radio" value="awesome" required />
                                                    <label for="awesome">Sim</label>
                                                </div>
                                                <div class="radio-custom radio-primary">
                                                    <input id="very-awesome" name="porto_is" type="radio" value="very-awesome" />
                                                    <label for="very-awesome">Não</label>
                                                </div>
                                                <label class="error" for="porto_is"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <footer class="panel-footer">
                                        <div class="row">
                                            <div class="col-md-9 col-md-offset-3">
                                                <button type="submit" class="btn btn-success pull-right" style="margin-left: 5px;"><i class="fa fa-save"></i> &nbsp;&nbsp;Salvar</button>

                                            </div>
                                        </div>
                                    </footer>
                                </section>
                            }
                        </div>
                    </div>
