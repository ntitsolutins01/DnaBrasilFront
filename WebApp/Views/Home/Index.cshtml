@section Scripts {
	<script src="https://code.highcharts.com/highcharts.js"></script>

	<script>
		$(function () {

			Highcharts.chart('container', {

				chart: {
					type: 'column',
					inverted: false
				},

				title: {
					text: undefined
				},

				xAxis: {
					categories: ['Hoje', 'Última semana', 'Última mês']
				},

				yAxis: {
					title: {
						text: 'Laudos'
					}
				},

				plotOptions: {
					columnrange: {
						dataLabels: {
							enabled: true,
							formatter: function () {
								return this.y;
							}
						}
					}
				},

				legend: {
					enabled: false
				},

				series: [{
					name: 'Total',
					data: [
						{ y: 20.1, color: '#3949AB' },
						{ y: 34.4, color: '#8E24AA' },
						{ y: 11.7, color: '#1E88E5' }
					]
				}]

			});

		});


	</script>
}