                                title="periodoSelect" required
                                @* asp-for="CodPeriodo" asp-items="Model.ListPeriodo" *@>
                            <option value="">Selecionar Periodo</option>
                        </select>
                    </div>

					<label class="col-sm-2 control-label">Estado:</label>
                    <div class="col-md-4 form-group">
                        <select class="form-control populate select2" name="ddlEstado" id="ddlEstado"
                                title="estadoSelect" required
