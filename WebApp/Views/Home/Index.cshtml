					<label class="col-sm-2 control-label">Cidade:</label>
                    <div class="col-md-4 form-group">
                        <select class="form-control populate select2" name="ddlCidade" id="ddlCidade"
                                title="cidadeSelect" required
                                @* asp-for="CodCidade" asp-items="Model.ListCidade" *@>
                            <option value="">Selecionar Cidade</option>
                        </select>
                    </div>

					<label class="col-sm-2 control-label">Localidade:</label>
                    <div class="col-md-4 form-group">
                        <select class="form-control populate select2" name="ddlLocalidade" id="ddlLocalidade"
                                title="localidadeSelect" required
                                @* asp-for="CodLocalidade" asp-items="Model.ListLocalidade" *@>
                            <option value="">Selecionar Localidade</option>
                        </select>
                    </div>

				</div>
				<footer class="panel-footer">
					<div class="row">
						<div class="col-md-9 col-md-offset-3">
							<button type="submit" class="btn btn-primary pull-right" style="margin-left: 5px;margin-right: 5px;"><i class="fa fa-search"></i> &nbsp;&nbsp;Pesquisar</button>
