@model DashboardModel
@{
    ViewData["Title"] = "Dashboard";
}

<div class="col-md-6 col-lg-12 col-xl-6">
    <div class="row">
		<form id="formDashboard" action="forms-validation.html" class="form-horizontal">
			<section class="panel">
				<header class="panel-heading">
					<div class="panel-actions">
						<a href="#" class="panel-action panel-action-toggle" data-panel-toggle></a>
					</div>
					<h2 class="panel-title">Filtro de pesquisa</h2>
				</header>
				<div class="panel-body">
					<label class="col-sm-2 control-label">Periodo:</label>
                    <div class="col-md-4 form-group">
                        <select class="form-control populate select2" name="ddlPeriodo" id="ddlPeriodo"
                                title="periodoSelect">
	                        <option value="">Selecionar Periodo</option>
	                        <option value="matutino">MATUNINO</option>
	                        <option value="vespertino">VESPERTINO</option>
	                        <option value="noturno">NOTURNO</option>
                        </select>
                    </div>

					<label class="col-sm-2 control-label">Estado:</label>
                    <div class="col-md-4 form-group">
                        <select class="form-control populate select2" name="ddlEstado" id="ddlEstado"
                                title="estadoSelect" required
                                asp-for="EstadoId" asp-items="Model.ListEstados">
                            <option value="">Selecionar Estado</option>
                        </select>
                    </div>

					<label class="col-sm-2 control-label">Cidade:</label>
                    <div class="col-md-4 form-group">
                        <select class="form-control populate select2" name="ddlCidade" id="ddlCidade"
                                title="cidadeSelect" required
                                asp-for="CidadeId" asp-items="Model.ListCidades">
                            <option value="">Selecionar Cidade</option>
                        </select>
                    </div>

					<label class="col-sm-2 control-label">Localidade:</label>
                    <div class="col-md-4 form-group">
                        <select class="form-control populate select2" name="ddlLocalidade" id="ddlLocalidade"
                                title="localidadeSelect" required
                                asp-for="LocalidadeId" asp-items="Model.ListLocalidades">
                            <option value="">Selecionar Localidade</option>
                        </select>
                    </div>

				</div>
				<footer class="panel-footer">
					<div class="row">
						<div class="col-md-9 col-md-offset-3">
							<button type="submit" class="btn btn-primary pull-right" style="margin-left: 5px;margin-right: 5px;"><i class="fa fa-search"></i> &nbsp;&nbsp;Pesquisar</button>
						</div>
					</div>
				</footer>
			</section>
		</form>

