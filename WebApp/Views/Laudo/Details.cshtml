@using System.Globalization
@model LaudoModel
@{
    ViewData["Title"] = "Laudo Detalhado";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>@Model.Aluno.Nome</h1>
<br />
<div id="vLaudoDetails">
    <div class="col-md-11 col-lg-12">
        <div class="col-md-12">
            <div class="panel-group" id="accordion2">
                @* Saude -----------------------------------------------------------------------------------------------------------*@
                @if (Model.Laudo.SaudeId != null)
                {
                    <div class="panel panel-accordion panel-accordion-danger">
                        <div class="panel-heading">
                            <h4 class="panel-title">
                                <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapse2One">
                                    <i class="fa fa-heartbeat"></i> Saúde
                                </a>
                            </h4>
                        </div>
                        <div id="collapse2One" class="accordion-body collapse">
                            <div class="panel-body">
                                <div class="well well-sm">
                                    <div class="form-group">
                                        <div class="col-sm-2" style="width: 10% !important;">
                                            <div class="liquid-meter-wrapper liquid-meter-sm">
                                                <div class="liquid-meter">
                                                    <meter min="0" max="100" value="@Model.Desempenho.ScoreSaude" id="meterSaude"></meter>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-10">
                                            @{
                                                string dataFormatada = Model.Desempenho.DataSaude?.ToString("d", CultureInfo.CreateSpecificCulture("pt-BR"));
                                            }
                                            <div class="h4 text-weight-bold mb-3" style="margin-top: 30px;">
                                                Data de realização do teste em @dataFormatada
                                            </div>
                                            <p>Porcentagem de saúde</p>
                                        </div>
                                    </div>

                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        <section class="panel">
                                            <div class="panel-body">
                                                <table class="table table-bordered table-striped mb-0" id="datatable-default">
                                                    <tbody>
                                                        <tr>
                                                            <th class="text" style="font-weight: bold;">Profissional</th>
                                                            <td class="text-center">@Model.Profissional.Nome</td>
                                                            <th class="text" style="font-weight: bold;">Idade</th>
                                                            <td class="text-center">@Model.Aluno.Idade</td>
                                                            <th class="text" style="font-weight: bold;">Sexo</th>
                                                            <td class="text-center">@Model.Aluno.Sexo</td>
                                                        </tr>
                                                    </tbody>
                                                    <tbody>
                                                        <tr>
                                                            <th class="text" style="font-weight: bold;">Local</th>
                                                            <td class="text-center">@Model.Aluno.NomeLocalidade</td>
                                                            <th class="text" style="font-weight: bold;">Série</th>
                                                            <td class="text-center">Não Definido</td>
                                                            <th class="text" style="font-weight: bold;">Turma</th>
                                                            <td class="text-center">Não Definido</td>
                                                        </tr>
                                                    </tbody>
                                                    <tbody>
                                                        <tr>
                                                            <th class="text" style="font-weight: bold; width: 20%">Altura</th>
                                                            <td class="text-center">@Model.Laudo.Estatura</td>
                                                            <th class="text" style="font-weight: bold; width: 20%">Peso</th>
                                                            <td class="text-center">@Model.Laudo.Massa</td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                                <div class="form-group">
                                                    <div class="col-sm-4">
                                                        <div class="row form-group">
                                                            <a data-toggle="modal" data-target="#mdDetalhamento">
                                                                <section class="panel col-md-12">
                                                                    <div class="panel-body bg-danger-less">
                                                                        <div class="circular-bar circular-bar-xs m-none mt-xs mr-md pull-right">
                                                                            <div class="circular-bar-chart" data-percent="@Model.Laudo.ImcSaude" data-plugin-options='{ "barColor": "#546E7A", "delay": 300, "size": 50, "lineWidth": 5 }'>
                                                                                <strong>Average</strong>
                                                                                <label><span>@Model.Laudo.ImcSaude</span></label>
                                                                            </div>
                                                                        </div>
                                                                        <div class="h4 text-weight-bold mb-none">Indice de massa corporal (IMC)</div>
                                                                        <p class="text-xs mb-none">@Model.Desempenho.AvisoImc</p>
                                                                    </div>
                                                                </section>
                                                            </a>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-4">
                                                        <div class=" alert alert-info">
                                                            <strong>0%</strong>
                                                            <div>
                                                                <label class="control-label">Acima do Peso</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-4">
                                                        <div class=" alert alert-info">
                                                            <strong>0%</strong>
                                                            <div>
                                                                <label class="control-label">Baixo Peso</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </section>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                }

